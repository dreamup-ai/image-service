  "bugs": {
    "url": "https://github.com/dreamup-ai/image-service/issues"
  },
  "homepage": "https://github.com/dreamup-ai/image-service#readme",
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.3",
    "@types/sinon": "^10.0.14",
    "@types/uuid": "^9.0.1",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "sinon": "^15.0.4",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.326.0",
    "@aws-sdk/client-s3": "^3.326.0",
