    "@fastify/swagger-ui": "^1.8.1",
    "@fastify/type-provider-json-schema-to-ts": "^2.2.2",
    "dotenv": "^16.0.3",
    "dynamo-tools": "github:shawnrushefsky/dynamo-tools",
    "fastify": "^4.17.0",
    "json-schema-to-ts": "^2.8.0",
    "jsonwebtoken": "^9.0.0",
    "sharp": "^0.32.1",
    "uuid": "^9.0.0"
  }
}
